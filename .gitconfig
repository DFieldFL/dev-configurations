[user]  
        email = brandon.groves@gmail.com
        name = Brandon Groves
        signingKey = 
[core]
        autocrlf = false
        editor = vim
[push]
        default = simple
[alias]
        hist = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
        st = status -sb
        cm = commit
        co = checkout
        tags = tag -l
        branches = branch -a
        remotes = remote -v
        br = branch
        df = diff
        p = push
        po = push origin
        poh = push origin HEAD
        lg = log -p
        ft = fetch
        fu = fetch upstream
        fup = fetch upstream --prune
        fop = fetch origin --prune
        fap = fetch --all --prune
        rb = rebase
        sh = stash
        shop = stash pop
        rum = rebase upstream/master
        rom = rebase origin/master
        com = checkout master
        rb2 = rebase -i HEAD~2
[pager]
        diff = diff-so-fancy | less --tabs=1,5 -R
        show = diff-so-fancy | less --tabs=1,5 -R
[color "diff-highlight"]
        oldNormal = red bold
        oldHighlight = red bold 52
        newNormal = green bold
        newHighlight = green bold 22
[commit]
        gpgsign = true

